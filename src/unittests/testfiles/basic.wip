fun whileLoop(num i32) i32
{
    var i i32 = 0;
    var rv i32 = 1;
    while i < num
    {
        rv = rv * 2;
        i = i + 1;
    }

    rv
}

fun nestedLoop(num i32) i32
{
    var i i32 = 0;
    var rv i32 = 1;
    while i < num
    {
        rv = if i % 2 == 0 {rv * 2} else {rv * 3};

        var j i32 = 0;
        while j < i
        {
            rv = rv + 1;
            j = j + 1;
        }

        i = i + 1;
    }

    rv
}

