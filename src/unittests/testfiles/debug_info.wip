fun noParams() i32
{
    0
}

fun param(param1 u8) u16
{
    param1
}

fun operators(x i32, y i64, z i8) i64
{
    -x + y / (x * z)
}

fun whileLoop(a i32, b i32) i32
{
    while a > 0
    {
        b *= 3;
        a -= 1;
    }

    b
}

fun types()
{
    var b bool   = true;
    var x0 i8    = 0;
    var x1 i16   = 1;
    var x2 i32   = 2;
    var x3 i64   = 3;
    var x4 isize = 4;
    var x5 u8    = 5;
    var x6 u16   = 6;
    var x7 u32   = 7;
    var x8 u64   = 8;
    var x9 usize = 9;
    var s str    = "abc";
}
